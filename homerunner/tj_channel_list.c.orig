#include "tj_channel_list.h"

struct _TJChannelListPrivate
{
	GtkWidget* box1;
	GtkWidget* comboboxtext1;
	GtkWidget* scrolledwindow1;
	GtkWidget* treeview1;
};

G_DEFINE_TYPE_WITH_PRIVATE (TJChannelList, tj_channel_list, GTK_TYPE_BOX);

static void
tj_channel_list_class_init (TJChannelListClass *k)
{
	GtkWidgetClass* widget_class;

	widget_class = GTK_WIDGET_CLASS (k);
	gtk_widget_class_set_template_from_resource (widget_class, "/org/thoughtjacked/homerunner/tj_channel_list.ui");
	gtk_widget_class_bind_template_child_private (widget_class, TJChannelList, box1);
	gtk_widget_class_bind_template_child_private (widget_class, TJChannelList, comboboxtext1);
	gtk_widget_class_bind_template_child_private (widget_class, TJChannelList, scrolledwindow1);
	gtk_widget_class_bind_template_child_private (widget_class, TJChannelList, treeview1);
}

static void
tj_channel_list_init (TJChannelList *widget)
{
	widget->priv = G_TYPE_INSTANCE_GET_PRIVATE (widget, TJ_TYPE_CHANNEL_LIST, TJChannelListPrivate);
	gtk_widget_init_template (GTK_WIDGET (widget));
}

GtkWidget *
tj_channel_list_new ()
{
	return g_object_new (TJ_TYPE_CHANNEL_LIST, NULL);
}
